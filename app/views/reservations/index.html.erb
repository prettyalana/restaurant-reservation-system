<p style="color: green"><%= notice %></p>

<h1>Reservations</h1>

<button>
  <%= link_to "New reservation", new_reservation_path %>
</button>

<div id="reservations">
  <% @reservations.each do |reservation| %>
    <%= render reservation %>
    <p>
      <%= link_to "Show this reservation", reservation %>
    </p>
  <% end %>
</div>
