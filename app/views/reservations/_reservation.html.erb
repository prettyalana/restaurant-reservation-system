<div id="<%= dom_id reservation %>">
  <p>
    <strong>Name:</strong>
    <%= reservation.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= reservation.description %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= reservation.date %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= reservation.time.strftime("%I:%M %p") %>
  </p>

</div>
